{
  "name": "blocklist-client",
  "scripts": {
    "build-openapi": "openapi-generator-cli generate -i ./openapi.json -g rust -o ../../../.generated-sources/blocklist --config ./config.json",
    "clean-openapi": "find ../../../.generated-sources/blocklist ! -path '../../../.generated-sources/blocklist/src/main.rs' ! -path '../../../.generated-sources/blocklist/Cargo.toml' -mindepth 1 -delete",
    "build": "pnpm build-openapi",
    "clean": "pnpm clean-openapi"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.2"
  }
}
